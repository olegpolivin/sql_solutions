# 1
SELECT PC.model, PC.speed, PC.hd
FROM PC
WHERE PC.price < 500;

# 2
SELECT DISTINCT maker
FROM Product
WHERE type='Printer';

# 3
SELECT model, ram, screen
FROM Laptop
WHERE price > 1000

# 4
SELECT *
FROM Printer
WHERE color = 'y';

# 5
SELECT model, speed, hd
FROM PC
WHERE (cd = '12x' OR cd = '24x') AND price < 600;

# 6
SELECT DISTINCT maker, speed FROM
Product JOIN Laptop
ON Product.model = Laptop.model
WHERE hd>=10




